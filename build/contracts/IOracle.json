  "source": "// SPDX-License-Identifier: MIT\npragma solidity ^0.8.0;\n\ninterface IOracle {\n    function requestUserData(uint256 userId) external;\n}\n\ncontract UserOrderAPI {\n    address public oracle;\n    address public owner;\n\n    // Event to emit API data\n    event UserDataFetched(uint256 userId, string response);\n\n    modifier onlyOwner() {\n        require(msg.sender == owner, \"Not authorized\");\n        _;\n    }\n\n    constructor(address _oracle) {\n        oracle = _oracle;\n        owner = msg.sender;\n    }\n\n    // Request user data from Oracle\n    function fetchUserData(uint256 userId) public onlyOwner {\n        IOracle(oracle).requestUserData(userId);\n    }\n\n    // Oracle will call this function with API response\n    function fulfillUserData(uint256 userId, string memory response) public {\n        require(msg.sender == oracle, \"Only Oracle can call this\");\n        emit UserDataFetched(userId, response);\n    }\n}\n",
